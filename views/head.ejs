<% include ./common.ejs %>
<!DOCTYPE html>
<html lang="ko">
  <head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta content="width=device-width, initial-scale=1.0" name="viewport">

    <title><%= title %></title>
    
    <!-- Favicons -->
    <link href="<%= IMG_URL %>/favicon.png" rel="icon">
    <link href="<%= IMG_URL %>/apple-touch-icon.png" rel="apple-touch-icon">
  
    <!-- Google Fonts -->
    <link href="https://fonts.googleapis.com/css?family=Open+Sans:300,300i,400,400i,600,600i,700,700i|Montserrat:300,400,500,600,700" rel="stylesheet">
  
    <!-- Vendor CSS Files -->
    <link href="<%= VENDOR_URL %>/aos/aos.css" rel="stylesheet">
    <link href="<%= VENDOR_URL %>/bootstrap/css/bootstrap.min.css" rel="stylesheet">
    <link href="<%= VENDOR_URL %>/bootstrap-icons/bootstrap-icons.css" rel="stylesheet">
    <link href="<%= VENDOR_URL %>/glightbox/css/glightbox.min.css" rel="stylesheet">
    <link href="<%= VENDOR_URL %>/swiper/swiper-bundle.min.css" rel="stylesheet">
  
    <!-- Template Main CSS File -->
    <link href="<%= CSS_URL %>/style.css" rel="stylesheet">
  </head>
  <body>
    <!-- ======= Header ======= -->
    <header id="header" class="fixed-top d-flex align-items-center header-transparent">
      <div class="container d-flex align-items-center">
  
        <h1 class="logo me-auto"><a href="<%= IMBUTLER_URL %>">IMBUTLER</a></h1>
        <!-- Uncomment below if you prefer to use an image logo -->
        <!-- <a href="index.html" class="logo me-auto"><img src="<%= IMG_URL %>/logo.png" alt="" class="img-fluid"></a>-->
  
        <nav id="navbar" class="navbar order-last order-lg-0">
          <ul>
            <li><a class="nav-link scrollto" href="<%= IMBUTLER_URL %>#hero">Home</a></li>
            <li><a class="nav-link scrollto" href="<%= IMBUTLER_URL %>#about">About</a></li>
            <li><a class="nav-link scrollto" href="<%= IMBUTLER_URL %>#services">Services</a></li>
            <li><a class="nav-link scrollto " href="<%= IMBUTLER_URL %>#portfolio">Portfolio</a></li>
            <li><a class="nav-link scrollto" href="<%= IMBUTLER_URL %>#team">Team</a></li>
            <li><a class="nav-link scrollto" href="<%= IMBUTLER_URL %>/chat/room#chat">Chat</a></li>
            <% if(user) { %>
              <li class="dropdown"><a href="<%= IMBUTLER_URL %>#"><span><%=user?user.mb_name:''%>님</span> <i class="bi bi-chevron-down"></i></a>
                <ul>
                  <li><a href="#">회원정보</a></li>
                  <li><a href="<%= IMBUTLER_URL %>/member/logout">로그아웃</a></li>
                </ul>
              </li>
            <% } else { %>
              <li><a class="nav-link scrollto" href="<%= IMBUTLER_URL %>/member/login#sign-in">Login</a></li>
            <% } %>
          </ul>
          <i class="bi bi-list mobile-nav-toggle"></i>
        </nav><!-- .navbar -->
  
        <!-- <div class="social-links">
          <a href="<%= IMBUTLER_URL %>#" class="twitter"><i class="bi bi-twitter"></i></a>
          <a href="<%= IMBUTLER_URL %>#" class="facebook"><i class="bi bi-facebook"></i></a>
          <a href="<%= IMBUTLER_URL %>#" class="linkedin"><i class="bi bi-linkedin"></i></a>
          <a href="<%= IMBUTLER_URL %>#" class="instagram"><i class="bi bi-instagram"></i></a>
        </div> -->
  
      </div>
    </header><!-- End Header -->