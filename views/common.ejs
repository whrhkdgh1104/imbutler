<% DEBUG = 1 %>
<% HTTPS = true %>
<% URL = 'imbutler.herokuapp.com' %>

<% HTTP_PRE = 'http://' %>
<% if(HTTPS) HTTP_PRE = 'https://' %>

<% IMBUTLER_URL = HTTP_PRE + URL %>
<% if(DEBUG) IMBUTLER_URL = 'http://localhost' %>
<% CSS_URL = IMBUTLER_URL + '/assets/css' %>
<% JS_URL = IMBUTLER_URL + '/assets/js' %>
<% IMG_URL = IMBUTLER_URL + '/assets/img' %>
<% VENDOR_URL = IMBUTLER_URL + '/assets/vendor' %>